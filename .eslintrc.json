{
    "env": {
        "browser": 1
    },
    "globals": {
        "angular": 1,
        "$": 1,
        "jQuery": 1
    },  
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "extends": "eslint:recommended"
}