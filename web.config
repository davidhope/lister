<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <remove name="RewriteUserFriendlyURL1" />
                <remove name="RedirectUserFriendlyURL1" />
                <rule name="Listing">
                    <match url="^services/listing/([0-9]+)" />
                    <action type="Rewrite" url="services/listing.php?mls={R:1}" />
                </rule>
                <rule name="Listings">
                    <match url="services/listing" />
                    <action type="Rewrite" url="services/listing.php" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
